{
  "name": "rest-api-crud-mongodb",
  "version": "1.0.0",
  "description": "API CRUD operations using express(nodejs) and mongoDB",
  "main": "server.js",
  "jest": {
    "testMatch": [
      "**/tests/**"
    ]
  },
  "scripts": {
    "test": "NODE_ENV=test MONGODB_URL=mongodb://localhost:27017/test jest",
    "start": "NODE_ENV=prod node server.js",
    "dev": "NODE_ENV=dev MONGODB_URL=mongodb://localhost:27017/dev nodemon server.js",
